              <div class="d-flex justify-content-center align-items-center my-3">
                <div class="prev-tours"><i class="ri-arrow-left-line"></i></div>
                <div class="swiper-pagination"></div>
                <div class="next-tours"><i class="ri-arrow-right-line"></i></div>
              </div>



